    "mobx-utils": "^6.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.27.0",
    "@eslint/js": "^9.36.0",
    "@graphql-codegen/cli": "^6.0.0",
